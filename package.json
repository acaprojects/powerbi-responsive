    "concurrently": "^3.5.0",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.5.0",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "powerbi-client": "^2.3.5"
