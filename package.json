{
  "name": "powerbi-responsive",
  "version": "1.0.0",
  "description": "Embed-able, responsive reports from PowerBI",
  "repository": "https://github.com/acaprojects/powerbi-responsive",
  "main": "./dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
